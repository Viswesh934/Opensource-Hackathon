{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension.",
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["index.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["index.html", "index.js", "styles.css"],
        "matches": ["*://*/*"]
      }
    ]
  }
  